    <com.roy.views.LensView
        android:id="@+id/lensViews"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:hapticFeedbackEnabled="true"
        android:visibility="invisible"
        tools:visibility="visible" />

